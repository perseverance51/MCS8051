 233   1        u8  xdata *pPWMxCR;
 234   1        
 235   1        if(PWM_id > PWM57)  return FAIL;  //id错误
 236   1        if(pwm_eno > 1) return FAIL;  //输出使能错误
 237   1        if(pwm_ini > 1) return FAIL;  //输出端的初始电平错误
